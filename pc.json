{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "https://raw.githubusercontent.com/Envy-y/FIT3179-A2/main/A2%20data.csv",
        "format": {"type": "csv"}
    },
    "title": "Placeholder",
    "width": 600,
    "height": 400,
    "transform": [
        {
            "calculate": "parseInt(datum['EGM per 1000 Adults'])", "as": "EGM Per 1000 Adults"
        },
        {
            "calculate": "parseInt(datum['EXP per adult'])", "as": "Loss Per Adult"
        },
        {
            "calculate": "parseInt(datum['SEIFA Score'])", "as": "SEIFA Score"
        },
         {
            "calculate": "parseInt(datum['Adults Per venue'])", "as": "Adults Per Venue"
        },
        {
            "filter": "datum['LGA Name'] != 'Unincorporated' & datum['EGM per 1000 Adults'] != null & datum['Loss Per Adult'] > 0"
        },
        {"window": [{"op": "count", "as": "index"}]},
        {
            "fold": ["EGM Per 1000 Adults", "Adults Per Venue", "Loss Per Adult", "SEIFA Score"], "as": ["key","value"]
        },
        {
            "joinaggregate": [
                {"op": "min", "field": "value", "as": "min"},
                {"op": "max", "field": "value", "as": "max"}
            ],
            "groupby": ["key"]
        },
        {
            "calculate": "(datum.value - datum.min) / (datum.max - datum.min)",
            "as": "norm_val"
        },
        {
            "calculate": "(datum.min + datum.max) / 2",
            "as": "mid"
        }
    ],
    "layer": [
        {
            "mark": {
                "type": "rule", 
                "color": "#ccc"
            },
            "encoding": {
                "x": {"field": "key","sort": ["SEIFA Score","EGM Per 1000 Adults", "Adults Per Venue", "Loss Per Adult"]},
                "detail": {"aggregate": "count"}
            }
        },
        {
        "mark": "line",
            "encoding": {
            "color": {"type": "nominal", "field": "Region"},
            "detail": {"type": "nominal", "field": "index"},
            "opacity": {"value": 0.3},
            "x": {"type": "nominal", "field": "key","sort": ["SEIFA Score","EGM Per 1000 Adults", "Adults Per Venue", "Loss Per Adult"]},
            "y": {"type": "quantitative", "field": "norm_val", "axis": null},
            "tooltip": [{
                "type": "quantitative",
                "field": "EGM per 1000 Adults"
            }, {
                "type": "quantitative",
                "field": "EXP per adult",
                "format": "$"
            }, {
                "type": "quantitative",
                "field": "SEIFA Score"
            },{
                "type": "nominal",
                "field": "LGA Name"
            }]
            }
        }
        ],
        "config": {
    "axisX": {"domain": false, "labelAngle": 0, "tickColor": "#ccc", "title": null},
    "view": {"stroke": null}
    }
}